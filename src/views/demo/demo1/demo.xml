<root>
    <main _id="main">
        <form>
            <!-- <formItem tag="text" type='tel' placeholder="请输入" prop="company" lable="客户名称"/>
            <formItem tag="textarea" required="true" placeholder="请输入" prop="addressDetail" lable="详细地址"/> -->
            <formItem tag="date" placeholder="请输入" prop="date" lable="时间"/>
            <formItem tag="daterange" placeholder="请输入" width="500" prop="date1" lable="时间1"/>
            <formItem tag="radio" placeholder="请输入" prop="xx" lable="xx" options='[{"label":"黄金糕","value":"0"},{"label":"双皮奶","value":"1"}]'/>
            <formItem tag="checkbox" placeholder="请输入" prop="yy" lable="yy" options='[{"label":"黄金糕","value":"0"},{"label":"双皮奶","value":"1"}]'/>
            <formItem tag="cascader" lable="地区" prop="zone"/>
            <formItem tag="zoom" placeholder="请选择" width="300" innerTextW="300" dialogW="900" prop="name2"  lable="名称2" search="客户账号,客户名称|accountNo,company"  tableCol="客户账号,客户名称,电话|accountNo,company,phone" action='/lmis-md-web/cusm/totalQuery.do'/>
            <!-- <formItem tag="select"  placeholder="请输入" prop="province" lable="省" action="/lmis-md-web/address/queryProvince.do" method="post"/> -->
            <!-- <formItem tag="select" placeholder="请输入" prop="city" lable="城市" options='[{"label":"黄金糕","value":"选项1"},{"label":"双皮奶","value":"选项2"}]'/> -->

            <!-- <formItem tag="text" maxlength="5" placeholder="请输入" prop="zipCode" lable="邮编"/>
            <formItem tag="text" disabled="true" placeholder="请输入" prop="codAccountNo" lable="代收款客户账号"/>
            <formItem tag="text" required='true' placeholder="请输入" prop="name" lable="联系人"/>
            <formItem tag="text" required='true' type="tel" prop="phone" lable="联系电话"/>
            <formItem tag="text" required='true' type="number" placeholder="请输入" prop="age" lable="年龄"/>
            <formItem tag="text" required='true' type="email" placeholder="请输入" prop="email" lable="Email"/> -->

            <button type="primary" _id="query" isQueryBtn="true" placeholder="查询"/>
            <button type="success" _id="add" placeholder="创建"/>
        </form>
        <table _id="query" action="/lmis-md-web/cusm/totalQuery.do" lazyLoad="true" size="20" method="get">
            <tableCol  prop="accountNo" lable="客户账号" align="left"/>
            <tableCol  prop="company" lable="客户名称" />
            <tableCol  prop="addressDetail" lable="详细地址"/>
            <tableCol  lable="操作">
                <button type="danger" _id="statusClose"  placeholder="冻结"/>
                <button type="danger" _id="statusOpen"  placeholder="开通"/>
                <button type="warning" _id="update" placeholder="更新"/>
            </tableCol>
        </table>
    </main>
    <alert _id="statusOpen|statusClose"  action='/lmis-md-web/cusm/statusOpen.do|/lmis-md-web/cusm/statusClose.do' method="post" tip="确定开通吗？|确定冻结吗？"/>
    <dialog _id="add|update"  action='/lmis-md-web/cusm/saveClientAdd.do|update' method="get" _isDisabledId="update" >
        <form>
            <formItem tag="zoom"  required='true' placeholder="请选择" prop="name2"  lable="名称2" search="客户账号,客户名称|accountNo,company"  tableCol="客户账号,客户名称,电话|accountNo,company,phone" action='/lmis-md-web/cusm/totalQuery.do'/>
            <formItem tag="zoom" required='true' isDisabled="true" placeholder="请选择" prop="name1"  lable="名称1" search="客户账号,客户名称|accountNo,company"  tableCol="客户账号,客户名称,电话|accountNo,company,phone" action='/lmis-md-web/cusm/totalQuery.do'/>
            <formItem tag="text" placeholder="请输入" prop="accountNo" lable="客户账号"/>
            <formItem tag="text" required='true' isDisabled="true" placeholder="请输入" prop="company" lable="客户名称"/>
            <formItem tag="cascader" lable="地区" isDisabled="true" prop="province1" action="/lmis-md-web/address/queryProvince.do"/>
            <formItem tag="textarea" placeholder="请输入" isDisabled="true" prop="addressDetail" lable="详细地址"/>
            <formItem tag="text" type="number" required='true' maxlength="2" placeholder="请输入" prop="name" lable="联系人"/>
            <formItem tag="text" type="tel" required='true' maxlength="11" placeholder="请输入" prop="phone" lable="联系电话"/>
            <formItem tag="text" placeholder="请输入" prop="zipCode" lable="邮编"/>
            <formItem tag="text" type="email" required='true'  placeholder="请输入" prop="email" lable="Email"/>
            <formItem tag="text" placeholder="请输入" prop="dcId" lable="所属DC"/>
            <formItem tag="select" placeholder="请输入" prop="status" lable="状态" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <formItem tag="select" placeholder="请输入" prop="industryId" lable="所属行业" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <formItem tag="select" placeholder="请输入" prop="projectId" lable="项目分类" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <formItem tag="select" placeholder="请输入" prop="xiangmu" lable="项目名称" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <formItem tag="select" placeholder="请输入" prop="businessDepartment" lable="事业部" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <formItem tag="select" placeholder="请输入" prop="parentAccountNo" lable="客户父账号" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <formItem tag="text" placeholder="请输入" prop="codAccountNo" lable="代收款客户账号"/>
            <formItem tag="select" placeholder="请输入" prop="belongsBody" lable="所属公司主体" options='[{"label":"冻结","value":"2"},{"label":"开通","value":"1"}]'/>
            <button type="warning" _id="query" placeholder="保存"/>
        </form>
        <table _id="tj" action="/lmis-md-web/cusm/totalQuery.do" size="10" method="get">
            <tableCol  prop="accountNo" lable="客户账号" align="left"/>
            <tableCol  prop="company" lable="客户名称" />
            <tableCol  prop="addressDetail" lable="详细地址"/>
            <tableCol  lable="操作">
                <HButton type="primary" _id="tjAdd"  placeholder="新增"/>
                <button type="danger" _id="statusClose"  placeholder="冻结"/>
                <button type="danger" _id="statusOpen"  placeholder="开通"/>
                <button type="warning" _id="ccc" placeholder="更新"/>
            </tableCol>
        </table>
    </dialog>
    <dialog _id="ccc|tjAdd" action='url' _isDisabledId="tjAdd">
        <form>
            <formItem tag="text" placeholder="请输入" prop="accountNo" lable="客户账号"/>
            <formItem tag="text" isDisabled="true" required='true' placeholder="请输入啦啦啦啦" prop="name1" lable="姓名"/>
            <formItem tag="date" required='true' type="date" prop="date11" placeholder="选择日期时间" lable="日期"/>
            <formItem tag="text" required='true' placeholder="请输入" prop="name" lable="联系人"/>
            <formItem tag="text" placeholder="请输入" prop="phone" lable="联系电话"/>
            <formItem tag="text" placeholder="请输入" prop="zipCode" lable="邮编"/>
        </form>
    </dialog>
</root>
